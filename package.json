{
  "name": "aad-ui",
  "version": "0.2.0",
  "private": true,
  "devDependencies": {
    "cross-env": "^5.1.4",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "react-scripts": "2.1.5"
  },
  "dependencies": {
    "connected-react-router": "^6.3.1",
    "email-validator": "^2.0.4",
    "history": "^4.6.3",
    "marked": "^0.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.4.0",
    "react-dom": "^16.3.0",
    "react-materialize": "^2.6.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.1",
    "slugify": "^1.3.4",
    "superagent": "^4.1.0",
    "superagent-promise": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env PORT=4100 react-scripts start",
    "build": "react-scripts build",
    "test": "cross-env PORT=4100 react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "jquery": true,
      "node": true,
      "es6": true,
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "no-use-before-define": [
        "error",
        {
          "variables": false
        }
      ],
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "arrow-body-style": [
        0
      ]
    }
  }
}
